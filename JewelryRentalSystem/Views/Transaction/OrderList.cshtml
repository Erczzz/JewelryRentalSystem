@model IEnumerable<JewelryRentalSystem.Models.Transaction>

@{
    ViewData["Title"] = "OrderList";
}

<h1>OrderList</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Carts)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Carts)
            </td>
        </tr>
}
    </tbody>
</table>
